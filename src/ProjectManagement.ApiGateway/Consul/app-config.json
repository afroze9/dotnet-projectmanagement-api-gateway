{
  "Auth0Settings": {
    "Authority": "https://teamly.us.auth0.com",
    "Audience": "projectmanagement"
  },
  "TelemetrySettings": {
    "Endpoint": "http://jaeger:4317",
    "ServiceName": "api-gateway",
    "ServiceVersion": "v1",
    "EnableAlwaysOnSampler": false,
    "SampleProbability": 0.2,
    "EnableConsoleExporter": false
  },
  "Consul": {
    "Token": "517597e5-51fc-52b1-ed29-faf052e8edb8",
    "Discovery": {
      "ServiceName": "api-gateway",
      "Scheme": "https",
      "Retry": {
        "Enabled": true,
        "MaxAttempts": 5
      }
    }
  },
  "management": {
    "endpoints": {
      "health": {
        "showdetails": "always"
      },
      "enabled": true,
      "actuator": {
        "exposure": {
          "include": [
            "*"
          ]
        }
      }
    }
  }
}
